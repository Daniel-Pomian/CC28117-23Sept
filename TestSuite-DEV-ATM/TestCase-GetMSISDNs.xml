  <con:testStep type="groovy" name="Groovy Script-FormatMSISDNs" id="c2762099-9b1c-44c5-a72d-7d3f9ac2ad28">
    <con:settings/>
    <con:config>
      <script>/*
 * This script format generated msisdns to format 0+number
 * Date: 14.07.2016
 * Autor: Michael Gordon
*/

 // Generate and format msisdna
 String msisdn1 = context.expand( '${#Project#MSISDN-chargeCustomer-OK}' ).toString().replace('49','0')
 String msisdn2 = context.expand( '${#Project#MSISDN-chargeCustomer-OK}' ).toString().replace('49','0')
 String msisdn11 = context.expand( '${#Project#MSISDN-chargeCustomer-OK}' ).toString().replace('49','0').substring(0,11)
 String localTime = context.expand( '${#TestSuite#LocalTime}' )
 
 
 
 // write in properties msisdns
 testRunner.testCase.setPropertyValue('outMSISDN-1',msisdn1)
 testRunner.testCase.setPropertyValue('outMSISDN-2',msisdn2)
 testRunner.testCase.setPropertyValue('outMSISDN-11Signs',msisdn11)
 // save local time
 testRunner.testCase.setPropertyValue('LocalTime',localTime)

 // wrote in properties trace-nrs. 
 testRunner.testCase.setPropertyValue('TraceNr-1','1'+context.expand( '${#Project#Random5}' )) 
 testRunner.testCase.setPropertyValue('TraceNr-2','1'+context.expand( '${#Project#Random5}' )) 
 testRunner.testCase.setPropertyValue('TraceNr-3','1'+context.expand( '${#Project#Random5}' ))</script>
    </con:config>
  </con:testStep>
