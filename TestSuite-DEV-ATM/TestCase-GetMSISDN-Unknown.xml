  <con:testStep type="groovy" name="Groovy Script-GetUnknownMSISDNs" id="34cd5761-aa94-4fa9-bdf4-812836e18c64">
    <con:settings/>
    <con:config>
      <script>/*
 * This script format generated msisdns to format 0+number
 * Date: 19101.07.2016
 * Autor: Michael Gordon
*/

 // Generate and format msisdna
 String sPrefix = context.expand( '${#Project#MOCK-Prefix-Piranha-StackService-CustomResponse}' ).toString().replace('49','0') 
   
 // set config from service provider id = 1 and brandid=1
 String sSP = context.expand( '${#Project#ChargeCustomer-1-SP}' ).toString().replace('SP','') 
 String sBrandID=context.expand( '${#Project#ChargeCustomer-1-BrandID}' )
  
 // write in properties formated msisdns
 testRunner.testCase.setPropertyValue('outMSISDNUnknown',sPrefix+'9999999')
 // msisdn from another tenant
 testRunner.testCase.setPropertyValue('outMSISDNForbiddenSP',sPrefix+'321'+sSP+sBrandID)
 // msisdn is deactivated in response from piranha mock
 testRunner.testCase.setPropertyValue('outMSISDNDeactivated',sPrefix+'323'+sSP+sBrandID)
 // msisdn is inactivated in response from piranha mock
 testRunner.testCase.setPropertyValue('outMSISDNInactive',sPrefix+'324'+sSP+sBrandID)
  // msisdn for ocs-error=1
 testRunner.testCase.setPropertyValue('outMSISDNOCSError', context.expand( '${#Project#MSISDN_OCS_Unknown}' ).toString().replace('49','0') )</script>
