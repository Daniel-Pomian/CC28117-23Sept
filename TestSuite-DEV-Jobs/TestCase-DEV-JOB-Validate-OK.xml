          <path>//Results[1]/ResultSet[1]/Row[1]/PERMANENT_ERRORS[1]/text()</path>
          <content>0</content>
          <allowWildcards>false</allowWildcards>
          <ignoreNamspaceDifferences>true</ignoreNamspaceDifferences>
