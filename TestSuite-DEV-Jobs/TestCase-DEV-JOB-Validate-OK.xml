      </con:assertion>
      <con:assertion type="XPath Match" id="e013c264-eb4c-43b5-bb7c-b3a88b73f191" name="Match content of [ITERATIONS]">
        <con:configuration>
          <path>//Results[1]/ResultSet[1]/Row[1]/ITERATIONS[1]>0</path>
          <content>true</content>
          <allowWildcards>false</allowWildcards>
          <ignoreNamspaceDifferences>true</ignoreNamspaceDifferences>
          <ignoreComments>true</ignoreComments>
        </con:configuration>
      </con:assertion>
      <con:assertion type="XPath Match" id="bfcc5304-59e5-44e1-b613-51ee5f6c577a" name="Match content of [TOTAL_COUNT]">
        <con:configuration>
          <path>//Results[1]/ResultSet[1]/Row[1]/TOTAL_COUNT[1]>0</path>
