  <con:testStep type="groovy" name="Groovy Script_setCounts" id="fff90cf8-5720-4feb-a146-dc69f31024ed">
    <con:settings/>
    <con:config>
      <script>/*
 * This script save counts from tables in test-case properties
 * Date: 15.01.2019
 * Autor: Michael Gordon
*/

// set  table names and counts to vars 
def currentSuffix = context.expand( '${#TestCase#currentSuffix}' )
def table = context.expand( '${DataSource_getTableNames#tableName}' )
def archive = context.expand( '${DataSource_getTableNames#archiveName}' )
def countTable = context.expand( '${JDBC Request_getCountfromTable#ResponseAsXml#//Results[1]/ResultSet[1]/Row[1]/COUNT[1]}' )
def countArchive = context.expand( '${JDBC Request_getCountfromArchive#ResponseAsXml#//Results[1]/ResultSet[1]/Row[1]/COUNT[1]}' )

// set values to test-case properties
testRunner.testCase.setPropertyValue( table + currentSuffix,  countTable )
testRunner.testCase.setPropertyValue( archive + currentSuffix, countArchive )</script>
    </con:config>
  </con:testStep>
  <con:testStep type="groovy" name="Groovy Script_validateDifferences" id="9e1fab0b-afff-41e7-83bf-6f0c6f80c329">
    <con:settings/>
    <con:config>
      <script>/*
 * This script validate counts  from topups tables. Data to be archived must be moved from table to archive tabel
 *  Formel: Count_before - Count_After=Archive_Count_after-Archive_Count_before
 * Date: 15.01.2019
 * Autor: Michael Gordon
*/

// set constants
String before='_before'
String after	='_after'

// set current values
def currentSuffix = context.expand( '${#TestCase#currentSuffix}' )

// validate count of table intrances only after archiving is processed
if ( currentSuffix==after){
	// read values for compare
	def tableName = context.expand( '${DataSource_getTableNames#tableName}' )
	def archiveName = context.expand( '${DataSource_getTableNames#archiveName}' )	
	def tableName_before =context.expand( '${#TestCase#'+tableName+before+'}' )
	def tableName_after =context.expand( '${#TestCase#'+tableName+after+'}' )
	def archiveName_before =context.expand( '${#TestCase#'+archiveName+before+'}' )
	def archiveName_after =context.expand( '${#TestCase#'+archiveName+after+'}' )
	
	// log data
	log.info('Compare counts in  tables:  ' + tableName + '  AND   '+ archiveName)
	log.info (tableName+  ':- contains before rows: ' + tableName_before  + ' - and after - '+ tableName_after )
	log.info (archiveName+  ':- contains before rows: ' + archiveName_before  + ' - and after - '+ archiveName_after )
	
	// compare
	assert  tableName_before.toInteger()-tableName_after.toInteger()==archiveName_after.toInteger()-archiveName_before.toInteger()
	log.info(tableName_before.toInteger()-tableName_after.toInteger()==archiveName_after.toInteger()-archiveName_before.toInteger()) 
	}</script>
    </con:config>
  </con:testStep>
